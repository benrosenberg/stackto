# prints 4, 3, 2, 1, 0, done!
set $flag [f];
set $count [5];
mark a;
    if [$flag] then
        goto b;
    # note that varying amounts of whitespace around tokens
    # gets formatted away by the syntax highlighter/formatter
    #
    #
    #
    set $count [$count        1 -];
    set $flag   [$count 0 =];
    output $count;
goto a;
mark b  ;
set $done_text ["done!"];
output $done_text;